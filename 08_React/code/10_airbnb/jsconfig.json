{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "baseUrl": "./",
    "moduleResolution": "Node",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components": [
        "src/components/*"
      ],
      "@views": [
        "src/views/*"
      ],
      "@utils": [
        "src/utils/*"
      ],
      "@css": [
        "src/assets/css"
      ],
      "@img": [
        "src/assets/img"
      ]
    },
    "jsx": "preserve",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "ScriptHost"
    ]
  }
}